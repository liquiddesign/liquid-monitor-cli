{
    "name": "liquiddesign/liquid-design-cli",
    "type": "project",
    "require-dev": {
        "php": "^8.2.0",
        "laravel-zero/framework": "^11.36.1",
        "nunomaduro/laravel-console-menu": "^3.5",
        "laravel/pint": "^1.18.3",
        "mockery/mockery": "^1.6.12",
        "pestphp/pest": "^3.7.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "bin": ["builds/liquid-monitor-cli"],
    "require": {
        "illuminate/http": "^11.5"
    }
}
